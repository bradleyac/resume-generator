{
  "routes": [
    {
      "route": "/resume/*",
      "allowedRoles": [
        "anonymous"
      ]
    },
    {
      "route": "/assets/*",
      "allowedRoles": [
        "anonymous"
      ]
    },
    {
      "route": "/api/GetResumeData",
      "allowedRoles": [
        "anonymous"
      ]
    },
    {
      "route": "/api/*",
      "allowedRoles": [
        "anonymous"
      ]
    },
    {
      "route": "/*",
      "allowedRoles": [
        "contributor"
      ]
    }
  ],
  "responseOverrides": {
    "401": {
      "statusCode": 302,
      "redirect": "/.auth/login/aad"
    }
  },
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/api/*"
    ]
  }
}