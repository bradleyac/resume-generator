{
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "routes": [
    {
      "route": "/index.html",
      "allowedRoles": [
        "contributor"
      ]
    },
    {
      "route": "/resume/*",
      "methods": [
        "get"
      ],
      "allowedRoles": [
        "anonymous"
      ]
    },
    {
      "route": "/posting/*",
      "allowedRoles": [
        "contributor"
      ]
    }
  ]
}